<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>wikipediaplus@xulforge.com</em:id>
    <em:name>WikipediaPlus (example)</em:name>
    <em:description>A simple userscript that makes some visual changes on Wikipedia pages.</em:description>
    <em:version>0.1</em:version>
    <em:creator>Jorge Villalobos</em:creator>
    <em:bootstrap>true</em:bootstrap>
    <em:type>2</em:type>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>17.0</em:minVersion>
        <em:maxVersion>26.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Mozilla Firefox for Android -->
    <em:targetApplication>
      <Description>
        <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
        <em:minVersion>17.0</em:minVersion>
        <em:maxVersion>26.*</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>
</RDF>
